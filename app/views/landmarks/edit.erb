<h2>Edit Historical Landmark</h2>
<%=@landmark.name%>

<form action="/landmarks/<%=@landmark.id%>" method="POST"
  <input id="hidden" type="hidden" name="_method" value="patch">

  <p>Change Name:</p>
  <input type="text" name="landmark[name]" id="landmark_name">
  <br>

  <p>Change Year Completed:</p>
  <input type="text" name="landmark[year_completed]" id="landmark_name">
  <br>

</form>

